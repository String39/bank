<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ListaClientesServlet</servlet-name>
  	<display-name>ListaClientesServlet</display-name>
  	<description></description>
  	<servlet-class>com.curso.web.ListaClientesServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ClientesService</servlet-name>
  	<display-name>ClientesService</display-name>
  	<description></description>
  	<servlet-class>com.curso.servicios.ClientesService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListaClientesServlet</servlet-name>
  	<url-pattern>/ListaClientes</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClientesService</servlet-name>
  	<url-pattern>/Clientes</url-pattern>
  </servlet-mapping>
</web-app>
